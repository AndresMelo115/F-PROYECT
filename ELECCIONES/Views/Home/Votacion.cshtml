@model IEnumerable<ELECCIONES.Models.PuestoElecto>

@{
    ViewData["Title"] = "Votacion";
}

<h1>Votacion</h1>

<table class=" table table-bordered"
      <tr>
@*
    <td>@Model.Cedula @ViewBag.ciudadano</td>*@
      </tr>

      <thead>
        <tr>
             <th>
@Html.DisplayNameFor(model => model.Nombre)
          </th>

        </tr>
      </thead>

    <tbody>
@foreach (var item in Model)
{
    <tr>
    <td>
    @Html.DisplayFor(model => item.Nombre)
    </td>

    <td>
    <a asp-action="ProcesoVotacion" asp-route-id="@item.IdPuestoE"  class="btn btn-info">Votar</a>
    </td>


    </tr>

}
    </tbody>

</table>